@model IEnumerable<PissanoApp.Models.Proveedor>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CategoriaProveedor.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FormaPago.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.razonSocial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.direccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.representanteVentas)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.telefono)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.movil)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.numeroCuenta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ruc)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CategoriaProveedor.nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FormaPago.nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.razonSocial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.representanteVentas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.telefono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.movil)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.numeroCuenta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ruc)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.proveedorId }) |
                @Html.ActionLink("Detalles", "Details", new { id = item.proveedorId }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.proveedorId })
            </td>
        </tr>
    }

</table>
