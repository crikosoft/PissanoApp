@model PissanoApp.ViewModels.RequerimientoViewModel

@{
    ViewBag.Title = "Create";
}

<h2>Requerimiento de Compra</h2>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Crear Requerimiento de Compra</h4>
        <hr />
        @Html.ValidationSummary(true)

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>

        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Cabecera</h5>
                <div class="ibox-tools">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                </div>
            </div>
            <div class="ibox-content">

                <div class="form-group">
                    <label class="font-noraml col-sm-2 control-label">Obra</label>
                    <div class="input-group col-sm-10">
                        <select data-placeholder="Elija la Obra..." class="chosen-select" style="width:350px;" tabindex="1">
                            <option value="">Seleccione</option>

                            @foreach (var item in Model.Obras)
                            {
                                <option value=@Html.DisplayFor(modelItem => item.id)>
                                    @Html.DisplayFor(modelItem => item.nombre)
                                </option>
                            }
                        </select>
                    </div>
                </div>
                <div class="form-group">
                    <label class="font-noraml col-sm-2 control-label">Prioridad</label>
                    <div class="input-group col-sm-10">
                        <select data-placeholder="Seleccione la Prioridad..." class="chosen-select" style="width:350px;" tabindex="2">
                            <option value="">Seleccione</option>

                            @foreach (var item in Model.Prioridades)
                            {
                                <option value=@Html.DisplayFor(modelItem => item.prioridadId)>
                                    @Html.DisplayFor(modelItem => item.nombre)
                                </option>
                            }

                        </select>
                    </div>
                </div>

                <div class="form-group">
                    <label class="font-noraml col-sm-2 control-label">Comentario</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" tabindex="3"> <span class="help-block m-b-none">Agregue algún comentario a considerar durante la Orden de Compra</span>
                        @Html.EditorFor(model => model.Requerimiento.fecha)
                        @Html.ValidationMessageFor(model => model.Requerimiento.fecha)                    
                    
                    </div>
                </div>


            </div>
        </div>

        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Materiales</h5>
                <div class="ibox-tools">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
